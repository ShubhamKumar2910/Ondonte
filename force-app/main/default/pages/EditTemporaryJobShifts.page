        <apex:includeScript value="{!URLFOR($Resource.Calendar, '/FullCalendarJS/jquery.ui.touch-punch.min.js')}" />
        <apex:includeScript value="{!URLFOR($Resource.EditTemporaryJobShifts)}" />
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/css/bootstrap.min.css" rel="stylesheet" />
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/js/bootstrap.bundle.min.js"></script>
        <style>
            body {
    
                font-size: 14px;
                background: rgba(235, 234, 232, 0.459) !important;
                font-family: "Lucida Grande", Helvetica, Arial, Verdana, sans-serif !important;
            }
    
            #external-events h4 {
                font-size: 16px;
                margin-top: 0;
                padding-top: 1em;
            }
    
            #external-events .fc-event {
                margin: 10px 0;
                cursor: pointer;
                padding: 5px;
                background-color: #5a6ccc !important;
                border: #5a6ccc !important;
            }
    
            .fc-day-grid-event {
                border-color: #000000 !important;
                background-color: #000000 !important;
            }
    
            .slds-scope a {
                color: #5a6ccc !important;
            }
    
            #external-events p {
                margin: 1.5em 0;
                font-size: 11px;
                color: #666;
            }
    
            #external-events p input {
                margin: 0;
                vertical-align: middle;
            }
    
            /* #calendar {
                    float: right;
                    width: 900px;
                } */
    
            .fc-content {
                background: #5a6ccc;
                color: white;
            }
    
            button:disabled,
            button[disabled] {
                background-color: #cccccc !important;
            }
    
            .circular-event-status {
                width: 20px;
                height: 20px;
                border-radius: 50%;
            }
    
            .vertical-center {
                margin: 0;
                position: absolute;
                top: 50%;
                -ms-transform: translateY(-50%);
                transform: translateY(-50%);
            }
        </style>
