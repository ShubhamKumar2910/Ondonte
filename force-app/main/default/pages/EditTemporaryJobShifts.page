                                            <fieldset class="slds-form-element">
                                                <legend class="slds-form-element__legend slds-form-element__label">
                                                    <abbr class="slds-required" title="required">*</abbr>Frequency</legend>
                                                <div class="slds-form-element__control">
                                                    <span class="slds-radio">
                                                        <input type="radio" id="weekly" value="weekly" name="shift-type" checked="true" />
                                                        <label class="slds-radio__label" for="weekly">
                                                            <span class="slds-radio_faux"></span>
                                                            <span class="slds-form-element__label">Weekly</span>
                                                        </label>
                                                    </span>
                                                    <span class="slds-radio">
                                                        <input type="radio" id="monthly" value="monthly" name="shift-type" />
                                                        <label class="slds-radio__label" for="monthly">
                                                            <span class="slds-radio_faux"></span>
                                                            <span class="slds-form-element__label">Monthly</span>
                                                        </label>
                                                    </span>
                                                </div>
                                            </fieldset>
                                        </div>
                                    </div>
                                    <div class="weekly-div slds-col slds-size_4-of-6">
