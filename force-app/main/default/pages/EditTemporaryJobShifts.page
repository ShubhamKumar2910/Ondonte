                                <div class="slds-form-element">
                                    <div class="slds-form-element__control">
                                        <div id="st-type">
                                            <b>Requested Staff Type: </b>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br/>
                        <div class="slds-grid slds-gutters">
                            <div class="slds-col">
                                <div class="slds-form-element">
                                    <div class="slds-form-element__control">
                                        <div id="sk-type">
                                            <b>Requested Special Skills: </b>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <footer class="slds-modal__footer">
                        <button class="close-modal slds-button slds-button_neutral">Close</button>
                        <!-- <button id="update-event" class="slds-button slds-button_destructive">Update Status</button> -->
                    </footer>
                </div>
            </section>
            <div class="slds-backdrop slds-backdrop_open"></div>
        </div>
    
        <!--Slot Modal -->
        <div id="slot-modal" style="display:none;">
