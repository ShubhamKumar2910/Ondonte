    <body class="slds-scope">
        <div class="slds-spinner_container" id="spinner">
            <div class="slds-spinner--brand  slds-spinner slds-spinner--large slds-is-relative" role="alert">
                <span class="slds-assistive-text">Loading</span>
                <div class="slds-spinner__dot-a"></div>
                <div class="slds-spinner__dot-b"></div>
            </div>
        </div>
        <div id="wrap" class="slds-grid slds-gutters" style="padding: 1% !important;">
            <div id="external-events" class="slds-col slds-size_1-of-4">
                <article class="slds-card">
                    <div class="slds-card__header slds-grid">
                        <header class="slds-media slds-media_center slds-has-flexi-truncate">
                            <div class="slds-media__figure">
                                <span class="slds-icon_container slds-icon-standard-task" title="task">
                                    <svg class="slds-icon slds-icon_small" aria-hidden="true" style="background: #5a6ccc;">
                                        <use xlink:href="{!URLFOR($Asset.SLDS, 'assets/icons/standard-sprite/svg/symbols.svg#user')}"></use>
                                    </svg>
                                    <span class="slds-assistive-text">Manage day's avilable</span>
                                </span>
                            </div>
                            <div class="slds-media__body">
                                <h2 class="slds-card__header-title">
                                    <a href="javascript:void(0);" class="slds-card__header-link slds-truncate" title="Slots">
                                        <span>{!requisitionRecord.Name}</span>
                                    </a>
                                </h2>
                            </div>
                            <div id="shift-info" class="slds-no-flex" style="cursor:pointer;">
                                <svg class="slds-icon slds-icon_small" aria-hidden="true" style="fill:#5a6ccc;">
                                    <use xlink:href="{!URLFOR($Asset.SLDS, 'assets/icons/utility-sprite/svg/symbols.svg#info')}"></use>
                                </svg>
                            </div>
                        </header>
                    </div>
                    <div class="slds-card__body slds-card__body_inner">
                        <button id="btn-edit-slot" class="slds-button slds-button_success">Create New Shift</button>
                        <button id="upsert-shifts" class="slds-button slds-button_success" style="cursor:pointer !important;display:none;">Save Changes</button>
                    </div>
                </article>
            </div>
            <div id="cal-container-div" class="slds-col slds-size_3-of-4">
    
                <article class="slds-card" id="calendar-card">
                    <div class="slds-card__header slds-grid">
                        <header class="slds-media slds-media_center slds-has-flexi-truncate">
                            <div class="slds-media__figure">
                                <span class="slds-icon_container slds-icon-standard-task" title="Calendar">
                                    <svg class="slds-icon slds-icon_small" aria-hidden="true" style="background: #5a6ccc;">
                                        <use xlink:href="{!URLFOR($Asset.SLDS, 'assets/icons/standard-sprite/svg/symbols.svg#date_input')}"></use>
                                    </svg>
                                    <span class="slds-assistive-text">Calendar</span>
                                </span>
                            </div>
                            <div class="slds-media__body">
                                <h2 class="slds-card__header-title">
                                    <a href="javascript:void(0);" class="slds-card__header-link slds-truncate" title="Calendar">
                                        <span>Calendar</span>
                                    </a>
                                </h2>
                            </div>
                        </header>
                    </div>
                    <div class="slds-card__body slds-card__body_inner" id="no-shifts" style="display:none;">
                        <div class="slds-text-heading_small">There are no Open shift's assigned to "{!requisitionRecord.Name}" starting today.</div>
                    </div>
                    <div class="slds-card__body slds-card__body_inner" id="calendar-container">
                        <div id='calendar'></div>
                    </div>
                </article>
    
    
            </div>
        </div>
        <!-- Modal -->
        <div id="event-modal" style="display:none;">
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1"
